<?oxygen RNGSchema="http://www.tei-c.org/release/xml/tei/custom/schema/relaxng/tei_all.rng" type="xml"?>
<TEI xmlns="http://www.tei-c.org/ns/1.0">
    <teiHeader>
        <fileDesc>
            <titleStmt>
                <title>Authority List of Persons attested in MedCyprus</title>
            </titleStmt>
            <publicationStmt><ab/></publicationStmt>
            <sourceDesc><ab/></sourceDesc>
        </fileDesc>
    </teiHeader>
    <text>
        <body>
            <div type="commentary">
                <listPerson>
                    
                    <person xml:id="AlexiosKomnenos">
                        <persName xml:lang="en"><forename>Alexios I</forename> <surname>Komnenos</surname></persName>
                        <persName xml:lang="grc"><forename>Ἀλέξιος</forename> <surname>Κομνηνός</surname></persName>       
                        <birth  notBefore="1050" notAfter="1057">Around 1057</birth>
                        <death when="1118">1118</death>                       
                        <floruit from="1081" to="1118">reign1081-1118</floruit>
                        <occupation type="office-secular">emperor</occupation>
                        <note>The church of Panagia Phorbiotissa or Asinou, Niketari was constructed and first decorated with wall-paintings during his reign (acknowledged in the donor's inscription).</note>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q41600</idno>
                        <idno type="PBW">http://pbw2016.kdl.kcl.ac.uk/person/Alexios/1/</idno>
                    </person>
                    
                    <person xml:id="AnastasiaSaramalyna">
                        <persName xml:lang="en"><forename>Anastasia</forename> <surname>Saramalyna</surname></persName>
                        <persName xml:lang="grc"><forename>Ἀναστασία</forename> <surname>Σαραμαλύνα</surname></persName>
                        <floruit notBefore="1201" notAfter="1300">13th century</floruit>                
                        <note>Donor of the mural panel of St Anastasia the Deliverer from Poisons in the narthex of Panagia Phorbiotissa or Asinou, Niketari</note>
                    </person>
                    
                    <person xml:id="Eirene">
                        <persName xml:lang="en"><forename>Eirene</forename></persName>
                        <persName xml:lang="grc">Εἰρήνη</persName>
                        <floruit notBefore="1201" notAfter="1300">13th century</floruit>                     
                        <note>Founder of the church of Panagia, Moutoullas, and donor of its 1280 murals</note>
                    </person>
                    <relation active="#Eirene" ref="snap:wifeOf" passive="#IoannesGerakiotes">
                        <desc>wife of Ioannes Gerakiotes</desc>
                    </relation>
                    
                    <person xml:id="EumathiosPhilokales">
                        <persName xml:lang="en"><forename>Eumathios</forename> <surname>Philokales</surname></persName>
                        <persName xml:lang="grc"><forename>Εὐμάθιος</forename> <surname>Φιλοκάλης</surname></persName>
                        <birth  notBefore="1050" notAfter="1070">Around the middle of the 11th century</birth>
                        <death notBefore="1118" notAfter="1150">After 1118</death>                        
                        <floruit from="1092" to="1118">1092-1118</floruit>
                        <occupation type="office-secular" xml:lang="en">doux (military governor) of Cyprus</occupation>
                        <occupation type="office-secular" xml:lang="grc">δοὺξ Κύπρου</occupation>
                        <occupation type="dignity" xml:lang="en">protonobelissimos</occupation>
                        <occupation type="dignity" xml:lang="grc">πρώτιστος νωβελλισίμων</occupation>
                        <note>Donor (construction and painted decoration) of the chapel of Holy Trinity, Monastery of St John Chrysostom, Koutsovendes</note>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q928648</idno>
                        <idno type="PBW">http://pbw2016.kdl.kcl.ac.uk/person/Eumathios/102/</idno>
                    </person>
                    
                    <person xml:id="Germanos">
                        <persName xml:lang="en"><forename>Germanos</forename></persName>
                        <persName xml:lang="grc"><forename>Γερμανός</forename></persName>
                        <note>hieromonk of the monastery of Saint Nicholas the Roof at Kakopetria</note>
                        <!--<floruit notBefore="..." notAfter="...">...</floruit>-->
                        <occupation type="occupation">hieromonk</occupation>
                        <!--<idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person>
                    
                    <person xml:id="Georgios">
                        <persName xml:lang="en"><forename>Georgios</forename></persName>
                        <persName xml:lang="grc">Γεώργιος</persName>
                        <!--<note>...</note>
                        <floruit notBefore="..." notAfter="...">...</floruit>-->
                        <occupation type="occupation">priest</occupation>
                        <!--<idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person>
                    
                    <person xml:id="Ioannis">
                        <persName xml:lang="en"><forename>Ioannis</forename></persName>
                        <persName xml:lang="grc">Ιωάννης</persName>
                        <note>founder of the monastery of Saint Nicholas the Roof at Kakopetria</note>
                        <!--<floruit notBefore="..." notAfter="...">...</floruit>
                        <occupation type="occupation">...</occupation>
                        <idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person>
                    
                    <person xml:id="IoannesGerakiotes">
                        <persName xml:lang="en"><forename>Ioannes</forename> <surname>Gerakiotes</surname></persName>
                        <persName xml:lang="grc"><forename>Ἰωάννης</forename> <surname>Γερακιώτης</surname></persName>
                        <floruit notBefore="1201" notAfter="1300">13th century</floruit>                     
                        <note>Founder of the church of Panagia, Moutoullas, and donor of its 1280 murals</note>
                    </person>
                    <relation active="#IoannesGerakiotes" ref="snap:husbandOf" passive="#Eirene">
                        <desc>husband of Eirene</desc>
                    </relation>
                    
                    <person xml:id="Leon">
                        <persName xml:lang="en"><forename>Leon</forename> <surname>Authentes</surname></persName>
                        <persName xml:lang="grc">Λέων Αὐθέντης</persName>
                        <!--<note>...</note>
                        <floruit notBefore="..." notAfter="...">...</floruit>
                        <occupation type="occupation"></occupation>
                        <idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person>
                    <relation active="#Leon" ref="snap:sonOf" passive="#Authentes">
                        <desc>son of Authentes</desc>
                    </relation>
                    
                    <person xml:id="Authentes">
                        <persName xml:lang="en"><forename>Authentes</forename></persName>
                        <persName xml:lang="grc">Αὐθέντης</persName>
                        <!--<note>...</note>
                        <floruit notBefore="..." notAfter="...">...</floruit>
                        <occupation type="occupation"></occupation>
                        <idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person>
                    <relation active="#Authentes" ref="snap:fatherOf" passive="#Leon">
                        <desc>father of Leon</desc>
                    </relation>
                    
                    <person xml:id="Laurentios">
                        <persName xml:lang="en"><forename>Laurentios</forename></persName>
                        <persName xml:lang="grc">Λαυρέντιος</persName>
                        <!--<note>...</note>
                        <floruit notBefore="..." notAfter="...">...</floruit>
                        <occupation type="occupation"></occupation>
                        <idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person>
                    
                    <person xml:id="Neophytos">
                        <persName xml:lang="en"><forename>Neophytos</forename></persName>
                        <persName xml:lang="grc">Νεόφυτος</persName>
                        <note>hieromonk, ascetic and founder of the hermitage of Saint Neophytos at Tala</note>
                        <!--<floruit notBefore="..." notAfter="...">...</floruit>
                        <occupation type="occupation"></occupation>
                        <idno type="wikidata"></idno>
                        <idno type="PBW">...</idno>-->
                    </person> 
                    
                    <person xml:id="NikephorosIschyrios">
                        <persName xml:lang="en"><forename>Nikephoros</forename> <surname>Ischyrios</surname></persName>
                        <persName xml:lang="grc"><forename>Νικηφόρος</forename> <surname>Ἰσχύριος</surname></persName>    
                        <birth  notBefore="1034" notAfter="1067">Around the middle of the 11th century</birth>
                        <death notBefore="1106" notAfter="1133">Early 12th century</death>                   
                        <occupation type="dignity" xml:lang="en">magistros</occupation>                        
                        <occupation type="dignity" xml:lang="grc">μάγιστρος</occupation>
                        <note>Founder of the church of Panagia Phorbiotissa or Asinou, Niketari, and donor of its 1105/6 murals</note>
                    </person>
                    
                    <person xml:id="TheodoreApseudes">
                        <persName xml:lang="en"><forename>Theodore</forename> <surname>Apseudes</surname></persName>
                        <persName xml:lang="grc">Θεόδωρος Αψευδής</persName>
                        <floruit notBefore="1167" notAfter="1233">Late 12th-early 13th century</floruit>                        
                        <occupation type="occupation">painter</occupation>
                        <note>painter of the cell and bema (1183), Hermitage of Saint Neophytos, Tala</note>
                        <idno type="wikidata">https://www.wikidata.org/wiki/Q108041020</idno>
                       </person>
                    
                </listPerson>
            </div>
        </body>
    </text>
</TEI>