         <div type="edition" xml:lang="grc" xml:space="preserve">
<ab>
                
                <lb n="1"/><w lemma="ὑπέρ">ὑπὲρ</w> <w lemma="ὁ">τῆς</w> <persName type="emperor" key="nero"><name nymRef="Νέρων"><del rend="erasure">Νέρων</del>ος</name> 
                    <persName type="emperor" key="claudius"><name nymRef="Κλαύδιος">Κλαυδίου</name></persName>    
                    <lb n="2"/><name nymRef="Καῖσαρ">Καίσαρος</name></persName> <w lemma="νίκη">νίκης</w> <w lemma="καί">καὶ</w> 
                    <w lemma="σωτηρία">σωτηρίας</w> 
                    <lb n="3"/><w lemma="καί">καὶ</w> <w lemma="ὁ">τοῦ</w> <w lemma="οἶκος">οἴκου</w> <w lemma="αὐτός">αὐτοῦ</w> <w lemma="πᾶς">πάντος</w> 
