                    <lb n="3"/><w part="I">εὐτυχ</w> <gap reason="lost" extent="unknown" unit="character"/>
                    <lb n="4"/><persName type="attested"><name><seg type="F">λεον</seg></name></persName> <space quantity="1" unit="character"/><gap reason="lost" extent="unknown" unit="character"/>
