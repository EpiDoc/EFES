    <text>
        <body>
            <div type="commentary">
                <listPerson>
                    <person xml:id="Ιωάννης">
                        <persName xml:lang="en">Ioannis, founder of the monastery of Saint Nicholas the Roof at Kakopetria</persName>
                        <persName xml:lang="grc">Ιωάννης, κτήτωρ μονής Αγίου Νικολάου της Στέγης, Κακοπετριά</persName>
                    </person>
                    
                    <person xml:id="Γερμανός">
                        <persName xml:lang="en">Germanos, hieromonk of the monastery of Saint Nicholas the Roof at Kakopetria</persName>
                        <persName xml:lang="grc">Γερμανός, ιερομόναχος μονής Αγίου Νικολάου της Στέγης, Κακοπετριά</persName>
                    </person>
                    
                    <person xml:id="Νεόφυτος">
                        <persName xml:lang="en">Neophytos, hieromonk, ascetic and founder of the hermitage of Saint Neophytos at Tala</persName>
                        <persName xml:lang="grc">Νεόφυτος, ιερομόναχος, ασκητής και ιδρυτής Εγκλείστρας Αγίου Νεοφύτου, Τάλα</persName>
                    </person>
                    
                    <person xml:id="Λαυρέντιος">
                        <persName xml:lang="en">Laurentios</persName>
                        <persName xml:lang="grc">Λαυρέντιος</persName>
                    </person>    
                    
                    <person xml:id="TheodoreApseudes">
                        <persName xml:lang="en">Theodore Apseudes</persName>
                        <persName xml:lang="grc">Θεόδωρος Αψευδής</persName>
                        <note>painter of the cell and bema (1183), Hermitage of Saint Neophytos, Tala</note>
                        <floruit notBefore="1167" notAfter="1233">Late 12th-early 13th century</floruit>
                        <!--<floruit from="" to="">beginning and end dates of life/reign/etc.</floruit>-->
                        <occupation type="occupation">painter</occupation>
                        <idno type="Wikidata">https://www.wikidata.org/wiki/Q108041020</idno>
                        <idno type="PBW"></idno>
                    </person>
                    
                </listPerson>
            </div>
